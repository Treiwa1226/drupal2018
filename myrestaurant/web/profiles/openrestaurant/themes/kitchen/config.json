{
  "name": "Kitchen",
  "description": "The default administration theme for Open Restaurant.",
  "proxy" : "http://openrestaurant.dev/",
  "bowerDir": "./bower_components",
  "css": {
    "file" : "kitchen.style.scss",
    "src": [
      "assets/scss/**/*.scss"
    ],
    "dest": "assets/css"
  },
  "images": {
    "src": [
      "assets/images/**/*"
    ],
    "dest": "assets/images/"
  }
}
